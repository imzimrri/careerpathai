{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/vercel_handler.py": {
      "runtime": "@vercel/python@4.3.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/vercel_handler"
    }
  ]
}
